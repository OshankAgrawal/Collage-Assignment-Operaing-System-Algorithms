#include <stdio.h>
int fibonacci(int num){
    if(num==0)
        return num;
    else if(num==1)
        return num;
    else
        return fibonacci(num-1)+fibonacci(num-2);
}
int main(){
    int num;
    printf("Enter a number: ");
    scanf("%d",&num);
    printf("Fibonacci series is: ");
    for(int i=0;i<=num;i++){
        printf("%d\t",fibonacci(i));
    }
return 0;
}
